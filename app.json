{
  "expo": {
    "name": "Xar√°",
    "slug": "setophaga-expo",
    "version": "1.0.1",
    "scheme": "setophaga-expo",
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://xara-roan.vercel.app"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon-borda.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/xara-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFBF7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-borda.png",
        "backgroundColor": "#FFFBF7"
      },
      "package": "com.pedrovrima.setophagaexpo"
    },
    "extra": {
      "router": {
        "origin": "https://xara-roan.vercel.app"
      },
      "eas": {
        "projectId": "0d3d31fb-03c8-4fea-a60d-9b6742df408c"
      }
    }
  }
}
